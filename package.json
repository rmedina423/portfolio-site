{
  "name": "portfolio-site",
  "version": "0.0.1",
  "description": "This is a general portfolio layout project using react",
  "main": "index.js",
  "author": "Ryan M. Medina",
  "license": "MIT",
  "scripts": {
    "deploy": "rm -rf ./dist && yarn webpack",
    "lint": "eslint -c eslint.config.json .",
    "jest": "NODE_ENV=test jest -c client/jestClientConfig.json",
    "test": "yarn jest && yarn lint",
    "tdd": "yarn jest -- --watch",
    "test:coverage": "yarn jest -- --coverage && yarn lint",
    "travis:coverage": "yarn jest -- --coverage --maxWorkers=4 && yarn lint",
    "test:cw": "yarn jest -- --coverage --watch && yarn lint",
    "sendCoverage": "echo 'you need to set up code coverage integration with your tool of choice!'",
    "webpack": "NODE_ENV='production' ./node_modules/webpack/bin/webpack.js --config webpack.prod.config.js",
    "dev": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.dev.config.js"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "codeclimate-test-reporter": "^0.5.0",
    "css-loader": "^0.28.9",
    "enzyme": "^2.9.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.7.0",
    "node-sass": "^4.7.2",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2"
  }
}
