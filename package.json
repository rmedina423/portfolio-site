{
  "name": "portfolio-site",
  "version": "0.0.1",
  "description": "This is a general portfolio layout project using react",
  "main": "index.js",
  "author": "Ryan M. Medina",
  "license": "MIT",
  "scripts": {
    "deploy": "rm -rf ./dist && yarn run webpack",
    "lint": "./node_modules/eslint/bin/eslint.js -c eslint.config.json .",
    "jest": "NODE_ENV=test node_modules/jest/bin/jest.js -c ./jest.config.json",
    "test": "yarn run jest && yarn run lint",
    "test:coverage": "yarn run jest --coverage && yarn run lint",
    "travis:coverage": "yarn run jest --coverage --maxWorkers=4 && yarn run lint",
    "test:cw": "yarn run jest --coverage --watch && yarn run lint",
    "sendCoverage": "echo 'you need to set up code coverage integration with your tool of choice!'",
    "webpack": "NODE_ENV='production' ./node_modules/webpack/bin/webpack.js --config webpack.prod.config.js",
    "dev": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.dev.config.js"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "codeclimate-test-reporter": "^0.5.0",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^22.1.4",
    "jest-enzyme": "^4.0.2",
    "node-sass": "^4.7.2",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "lodash.chunk": "^4.2.0",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "uuid": "^3.2.1"
  }
}
